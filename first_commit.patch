+    var title = (function(){
+      var titype = typeof config.title === 'object';
+      return config.title
+      ? '<h3 style="'+ (titype ? config.title[1] : '') +'">'+ (titype ? config.title[0] : config.title)  +'</h3>'
+      : '';
+    }());
+    
